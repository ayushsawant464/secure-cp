{
  "mode": "testing",
  "vpn": {
    "interface": "wg0",
    "config_path": "/etc/wireguard/exam.conf",
    "namespace": "exam_ns",
    "vpn_ip": "10.8.0.2/24",
    "vpn_gateway": "10.8.0.1",
    "kill_switch_enabled": true
  },
  "network": {
    "allowed_domains": [
      "codeforces.com",
      "*.codeforces.com",
      "codeforces.org",
      "*.codeforces.org",
      "cf-static.com"
