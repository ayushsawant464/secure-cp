    "exam_app": {
      "name": "google-chrome",
      "path": "/usr/bin/google-chrome",
      "checksum": ""
    }
  },
  "security": {
    "use_apparmor": true,
    "profile_name": "exam-lockdown",
    "kernel_module_path": "/home/savvy19/Desktop/product/secure-exam-system/process_manager/kernel_monitor/proc_monitor.ko",
    "integrity_check_enabled": true
  },
  "logging": {
    "log_dir": "/var/log/secure-exam",
    "log_level": "INFO",
    "log_to_kernel": true
  }
